//IT21361036_Feedback
#include<iostream>
#include<string>
#include<cstring>

using namespace std;

class Feedback
{
private:
	int FeedbackID;
	char email[10];
	char mssg[50];

public:
	Feedback() {};
	Feedback(int fID, char fEmail[30], char fMssg[50]);
	void setFeedback(int FID, char Femail[30], char FMssg[60]);
	void display();
	int getFeedbackDetails();
};

Feedback::Feedback()
{
	int FeedbackID = 0;
	char email;
	char mssg;
}
Feedback::Feedback(int fID, char fEmail[30], char fMssg[50])
{
	fID = FeedbackID;
	fEmail = email;
	fMssg = mssg;
}
void Feedback::setFeedback(int FID, char*Femail, char*FMssg)
{
	FID = FeedbackID;
	Femail = email;
	FMssg = mssg;
}
int Feedback::getFeedbackDetails()
{
	return FeedbackDetails;
}
void Feedback::display()
{
	cout << "Feedback ID :" << FeedbackID << endl;
	cout << "Email Address :" << email << endl;
	cout << "Feedback :" << mssg << endl;
}
